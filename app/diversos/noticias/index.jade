extends ../base.jade

block home
  - var home_path = "../../"
  
block stylesheet
  - var stylesheet = "graduacao.min.css"
  
block page-title
  - page_title = "Est√°gio - " + page_title

block breadcrumbs-path
  -
    breadcrumbs_path = {}
    breadcrumbs_path[strings.breadcrumbs.home] = home_path
    breadcrumbs_path[strings.breadcrumbs.diversos] = "nolink"
    breadcrumbs_path[strings.breadcrumbs.noticias] = false

block content

  .graduacao-programa
    .container
      .session
        h1.session-title
          != strings.diversos.noticias.noticias
           
        p.session-subtitle
          != strings.diversos.noticias.noticiasEInformacoes
        p
          ul
            li
              a(href="http://www.usp.br/agen/" target="_blank")
                span.field-body  
                  != strings.diversos.noticias.agenciaUSP
              span(class="external-link glyphicon glyphicon-new-window") 
            li
              a(href="http://www.poli.usp.br/pt/comunicacao/noticias.html" target="_blank")
                span.field-body  
                  != strings.diversos.noticias.servicoComunicacao
